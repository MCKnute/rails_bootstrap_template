<h2>Paid Invoices</h2>
<table class="table table-striped">
	<thead>
		<tr>
			<td>Client Name</td>
			<td>Amount Paid</td>
			<td>Date Paid</td>
		</tr>
	</thead>
	<tbody>
		<% if @paidvoices %>
			<% @paidvoices.each do |paid| %></td>
		<tr>
			<td><%= paid.client.name %></td>
			<td><%= paid.amount %></td>
			<td><%= paid.paid_date %></td>
		</tr>
	</tbody>
	<% end %>
<% end %>
</table>